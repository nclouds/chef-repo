<VirtualHost *:80>
  ServerName <%= @server_name %>
  DocumentRoot <%= @graylog2_basedir %>/web/public
  RailsEnv 'production'
  <Directory <%= @graylog2_basedir %>/web/public>
    Allow from all
    Options -MultiViews
  </Directory>
  ErrorLog /var/log/apache2/error.log
  LogLevel warn
  CustomLog /var/log/apache2/access.log combined
</VirtualHost>

